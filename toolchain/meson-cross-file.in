[host_machine]
system = 'banan_os'
cpu_family = 'ARCH'
cpu = 'ARCH'
endian = 'little'

[binaries]
c = 'ARCH-pc-banan_os-gcc'
cpp = 'ARCH-pc-banan_os-g++'
ar = 'ARCH-pc-banan_os-ar'
ld = 'ARCH-pc-banan_os-ld'
